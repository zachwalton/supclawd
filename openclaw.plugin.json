{
  "id": "sup-chat",
  "name": "Sup Chat Channel",
  "version": "1.0.0",
  "description": "OpenClaw messaging channel for sup.net chat API",
  "kind": "channel",
  "channel": {
    "id": "sup-chat",
    "label": "Sup Chat",
    "selectionLabel": "Sup Chat (sup.net API)",
    "docsPath": "/channels/sup-chat",
    "docsLabel": "sup-chat",
    "blurb": "Connect to sup.net chat API for monitoring mentions/DMs and responding.",
    "order": 100,
    "aliases": ["sup"]
  },
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "baseUrl": {
        "type": "string",
        "description": "Base URL for the sup.net API"
      },
      "authSessionPath": {
        "type": "string",
        "description": "Path to file containing auth_session cookie value"
      },
      "clientVersion": {
        "type": "string",
        "description": "Client version header value"
      },
      "sessionId": {
        "type": "string",
        "description": "Session ID header value"
      },
      "pollInterval": {
        "type": "number",
        "description": "Interval in milliseconds to poll for new messages"
      },
      "enabled": {
        "type": "boolean",
        "description": "Enable/disable the channel"
      }
    }
  },
  "uiHints": {
    "baseUrl": {
      "label": "API Base URL",
      "placeholder": "https://sup.net"
    },
    "authSessionPath": {
      "label": "Auth Session Cookie File",
      "placeholder": "~/.config/sup/auth_session",
      "sensitive": true
    },
    "clientVersion": {
      "label": "Client Version",
      "placeholder": "1.0.0"
    },
    "sessionId": {
      "label": "Session ID",
      "placeholder": "optional-session-id"
    },
    "pollInterval": {
      "label": "Poll Interval (ms)",
      "placeholder": "5000"
    },
    "enabled": {
      "label": "Enabled"
    }
  }
}
